<h1>Listing geos</h1>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @geos.each do |geo| %>
      <tr>
        <td><%= geo.title %></td>
        <td><%= link_to 'Show', geo %></td>
        <td><%= link_to 'Edit', edit_geo_path(geo) %></td>
        <td><%= link_to 'Destroy', geo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Geo', new_geo_path, :class=>"btn btn-primary" %>
